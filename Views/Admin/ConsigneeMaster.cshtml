
@{
    ViewBag.Title = "ConsigneeMaster";
    Layout = "~/Views/Shared/_Layout.cshtml";
    @Scripts.Render("~/CustomScripts/ConsigneeMaster.js?t=" + @DateTime.Now.Ticks.ToString())
}

<input id="hf_GetConsigneeData" type="hidden" value='@Url.Action("GetConsigneeData",  "Admin")'>
<input id="hf_UpdateConsigneeDetails" type="hidden" value='@Url.Action("UpdateConsigneeDetails", "Admin")'>
<input id="hf_GetConsigneeDetails" type="hidden" value='@Url.Action("GetConsigneeDetails",  "Admin")'>
<input id="hf_ConsigneeInfoDelete" type="hidden" value='@Url.Action("ConsigneeInfoDelete", "Admin")'>
<div class="page-content">

    @*<div class="jumbotron content">*@
    <table id="tblConsignee" class="display" width="100%"></table>
    @*</div>*@
</div>
<div class="modal fade" id="myModalone" role="dialog">
    <div class="modal-dialog modal-large">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4>Add/Update Consignee Details</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div id="divContent" class="col-md-6">
                        <div class="form-group label-floating">
                            <label class="control-label">Name</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtName" placeholder="Enter Name">
                            </div>
                        </div>
                    </div>
                    <div id="divContent" class="col-md-6">
                        <div class="form-group label-floating">
                            <label class="control-label">Email Id</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtEmail" placeholder="Enter Email Id">
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div id="divContent" class="col-md-12">
                        <div class="form-group label-floating">
                            <label class="control-label">Address</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtAddress" placeholder="Enter Door No, Flat Name, Street Name">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div id="divContent" class="col-md-4">
                        <div class="form-group label-floating">
                            <label class="control-label">District</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtDistrict" placeholder="Enter District">
                            </div>
                        </div>
                    </div>

                    <div id="divContent" class="col-md-4">
                        <div class="form-group label-floating">
                            <label class="control-label">State</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtState" placeholder="Enter State">
                            </div>
                        </div>
                    </div>
                    <div id="divContent" class="col-md-4">
                        <div class="form-group label-floating">
                            <label class="control-label">Pincode</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm IsNumeric" id="txtPincode" placeholder="Enter Pincode">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div id="divContent" class="col-md-4">
                        <div class="form-group label-floating">
                            <label class="control-label">Phone No</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm IsNumeric" id="txtPhoneNo" placeholder="Enter Phone No">
                            </div>
                        </div>
                    </div>
                    <div id="divContent" class="col-md-4">
                        <div class="form-group label-floating">
                            <label class="control-label">Alternate Phone No</label>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm IsNumeric" id="txtAlternatePhoneNo" placeholder="Enter Phone No">
                            </div>
                        </div>
                    </div>
                    <div id="divContent" class="col-md-4">
                        <div class="form-group label-floating">
                            <label class="control-label">Alternate Email Id</label>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtAlternateEmailId" placeholder="Enter Email Id">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div id="divContent" class="col-md-6">
                        <div class="form-group label-floating">
                            <label class="control-label">User Name</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtUserName" placeholder="Enter User Name">
                            </div>
                        </div>
                    </div>
                    <div id="divContent" class="col-md-6">
                        <div class="form-group label-floating">
                            <label class="control-label">Password</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtPassword" placeholder="Enter Password">
                            </div>
                        </div>
                    </div>
                    
                </div>
            </div>
            <div class="modal-footer">
                <br />
                <button type="button" class="btn btn-default" id="btnUpdate">Save changes</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

