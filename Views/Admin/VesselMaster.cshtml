
@{
    ViewBag.Title = "VesselMaster";
    Layout = "~/Views/Shared/_Layout.cshtml";
    @Scripts.Render("~/CustomScripts/VesselMaster.js?t=" + @DateTime.Now.Ticks.ToString())
}

<input id="hf_GetVesselData" type="hidden" value='@Url.Action("GetVesselData",  "Admin")'>
<input id="hf_UpdateVesselDetails" type="hidden" value='@Url.Action("UpdateVesselDetails", "Admin")'>
<input id="hf_GetVesselDetails" type="hidden" value='@Url.Action("GetVesselDetails",  "Admin")'>
<input id="hf_VesselInfoDelete" type="hidden" value='@Url.Action("VesselInfoDelete", "Admin")'>
<div class="page-content">

    @*<div class="jumbotron content">*@
    <table id="tblVessel" class="display" width="100%"></table>
    @*</div>*@
</div>
<div class="modal fade" id="myModalone" role="dialog">
    <div class="modal-dialog modal-large">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4>Add/Update Vessel Details</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div id="divContent" class="col-md-6">
                        <div class="form-group label-floating">
                            <label class="control-label">Name</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtName" placeholder="Enter Name">
                            </div>
                        </div>
                    </div>
                   
                </div>

            </div>
            <div class="modal-footer">
                <br />
                <button type="button" class="btn btn-default" id="btnUpdate">Save changes</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

