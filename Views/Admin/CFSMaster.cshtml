
@{
    ViewBag.Title = "CFSMaster";
    Layout = "~/Views/Shared/_Layout.cshtml";
    @Scripts.Render("~/CustomScripts/CFSMaster.js?t=" + @DateTime.Now.Ticks.ToString())
}

<input id="hf_GetCFSData" type="hidden" value='@Url.Action("GetCFSData",  "Admin")'>
<input id="hf_UpdateCFSDetails" type="hidden" value='@Url.Action("UpdateCFSDetails", "Admin")'>
<input id="hf_GetCFSDetails" type="hidden" value='@Url.Action("GetCFSDetails",  "Admin")'>
<input id="hf_CFSInfoDelete" type="hidden" value='@Url.Action("CFSInfoDelete", "Admin")'>
<div class="page-content">

    @*<div class="jumbotron content">*@
    <table id="tblCFS" class="display" width="100%"></table>
    @*</div>*@
</div>
<div class="modal fade" id="myModalone" role="dialog">
    <div class="modal-dialog modal-large">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4>Add/Update CFS Details</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div id="divContent" class="col-md-6">
                        <div class="form-group label-floating">
                            <label class="control-label">CFS Location</label><em>*</em>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtLocation" placeholder="Enter Name">
                            </div>
                        </div>
                    </div>
                    <div id="divContent" class="col-md-6">
                        <div class="form-group label-floating">
                            <label class="control-label">CFS Code</label>
                            <div class="nk-int-st">
                                <input type="text" class="form-control input-sm" id="txtCode" placeholder="Enter Email Id">
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <br />
                <button type="button" class="btn btn-default" id="btnUpdate">Save changes</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

